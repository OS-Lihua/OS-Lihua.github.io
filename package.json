{
  "private": true,
  "name": "ossified-orbit",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "postinstall": "playwright install --with-deps chromium",
    "lint": "eslint . --ext .js,.ts,.astro --fix",
    "lint:check": "eslint . --ext .js,.ts,.astro",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "astro check && tsc --noEmit",
    "audit": "npm audit --audit-level moderate",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "clean-svg": "node scripts/clean-svg.js",
    "dev:clean": "yarn clean && yarn dev"
  },
  "dependencies": {
    "@astrojs/preact": "4.1.0",
    "@astrojs/rss": "4.0.12",
    "@astrojs/sitemap": "3.4.1",
    "@tailwindcss/vite": "^4.0.8",
    "@vercel/speed-insights": "^1.2.0",
    "alpinejs": "^3.14.9",
    "astro": "^5.11.0",
    "astro-icon": "^1.1.5",
    "katex": "^0.16.22",
    "playwright": "^1.54.1",
    "preact": "^10.26.2",
    "prismjs": "^1.30.0",
    "rehype-katex": "^7.0.1",
    "rehype-mermaid": "^3.0.0",
    "remark-math": "^6.0.0",
    "tailwindcss": "^4.0.8",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/upgrade": "^0.6.0",
    "@babel/core": "7.x",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "astro-eslint-parser": "^1.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.15.1",
    "tailwindcss-animated": "^2.0.0",
    "vite": "^6.0.0"
  },
  "engines": {
    "yarn": ">=1.22.0",
    "node": ">=16.0.0"
  },
  "resolutions": {
    "astro": "5.11.0"
  }
}
