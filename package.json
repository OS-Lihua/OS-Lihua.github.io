{
  "private": true,
  "name": "ossified-orbit",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "postinstall": "playwright install --with-deps chromium",
    "lint": "eslint . --ext .js,.ts,.astro --fix",
    "lint:check": "eslint . --ext .js,.ts,.astro",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "astro check && tsc --noEmit",
    "audit": "pnpm audit --audit-level moderate",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force -ErrorAction SilentlyContinue dist, .astro, node_modules/.cache\"",
    "clean-svg": "node scripts/clean-svg.js",
    "download-fonts": "node scripts/download-fonts.js",
    "analyze": "node scripts/analyze-bundle.js",
    "analyze:images": "node scripts/optimize-images.js",
    "analyze:css": "node scripts/analyze-css.js",
    "build:analyze": "pnpm build && pnpm analyze",
    "audit:performance": "node scripts/performance-audit.js",
    "dev:clean": "pnpm clean && pnpm dev"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "^6.3.3",
    "@astrojs/preact": "4.1.0",
    "@astrojs/rss": "4.0.12",
    "@astrojs/sitemap": "3.4.1",
    "@preact/compat": "^18.3.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@splinetool/react-spline": "^4.1.0",
    "@splinetool/runtime": "^1.10.71",
    "@tailwindcss/vite": "^4.1.11",
    "@vercel/speed-insights": "^1.2.0",
    "alpinejs": "^3.14.9",
    "astro": "^5.12.0",
    "astro-icon": "^1.1.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "katex": "^0.16.22",
    "lucide-preact": "^0.525.0",
    "playwright": "^1.54.1",
    "preact": "^10.26.9",
    "prismjs": "^1.30.0",
    "rehype-katex": "^7.0.1",
    "rehype-mermaid": "^3.0.0",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/upgrade": "^0.6.0",
    "@babel/core": "7.x",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "astro-eslint-parser": "^1.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss-animated": "^2.0.0",
    "vite": "^6.0.0"
  },
  "engines": {
    "pnpm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "pnpm": {
    "overrides": {
      "astro": "5.12.0"
    }
  }
}